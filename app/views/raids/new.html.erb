<fieldset>
  <% form_for(@raid) do |f| %>
  <%= f.error_messages %>
  
  <p><%= f.label :date, "Date" %> <%= date_select(:raid, :date) %></p>
  
  <p><%= f.label :attendance_output, "Attendance" %><br/>
  <%= f.text_area :attendance_output, :size => "125x30" %></p>
  
  <%= f.submit %>
  <% end %>
</fieldset>
<%- if admin? -%>
JuggyCompare_Data = {
  ["updated"] = "<%= Time.now.to_s(:long) %>",

  <%- for member in @members -%>
  ["<%= member.name %>"] = {
    ["NAME"]     = "<%= h(member.name) %>",
    ["RANK"]     = "<%= h(member.rank.name) unless member.rank_id.nil? %>",
    ["RAIDS_30"] = "<%= number_with_precision(member.attendance_30 * 100.0, :precision => 0) %>%",
    ["RAIDS_90"] = "<%= number_with_precision(member.attendance_90 * 100.0, :precision => 0) %>%",
    ["RAIDS_LT"] = "<%= number_with_precision(member.attendance_lifetime * 100.0, :precision => 0) %>%",
    ["LASTLOOT"] = "TODO: Last Loot",
    ["LF"]       = "<%= loot_factor(member.lf) %>",
    ["SLF"]      = "<%= loot_factor(member.sitlf) %>",
    ["BISLF"]    = "<%= loot_factor(member.bislf) %>",
  },
  <%- end -%>
}
<%- end -%>